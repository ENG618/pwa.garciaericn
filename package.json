    "start": "ng serve",
    "build": "ng build --prod",
    "postinstall": "ngcc",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "format:diff": "prettier --check \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && firebase deploy",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -s"
  },
  "private": true,
